<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

<bean id="customerDAO" class="com.github.Karina_Denisevich.spring_template.example.book.dao.impl.JdbcBookDao">
    <property name="dataSource" ref="dataSource"/>
</bean>

<bean id="customerSimpleDAO"
      class="com.github.Karina_Denisevich.spring_template.example.book.dao.impl.SimpleJdbcBookDAO">
    <property name="dataSource" ref="dataSource"/>
</bean>

</beans>